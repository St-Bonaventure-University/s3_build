{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": "*",
      "Action": ["s3:GetObject"],
      "Resource": ["arn:aws:s3:::{{ bucket_name }}/*"]
    },
    {
      "Effect": "Deny",
      "Principal": "{{ api_key }}",
      "Action": ["s3:DeleteObject"],
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Principal": "*",
      "Action": ["s3:ListBucket"],
      "Resource": ["arn:aws:s3:::{{ bucket_name }}"]
    },
    {
      "Effect": "Allow",
      "Principal": "*",
      "Action": ["s3:PutObject"],
      "Resource": ["arn:aws:s3:::{{ bucket_name }}/*"]
    },
    {
      "Effect": "Allow",
      "Principal": "*",
      "Action": ["s3:GetObject"],
      "Resource": ["arn:aws:s3:::{{ bucket_name }}/*"]
    },
    {
      "Effect": "Allow",
      "Principal": { "AWS": "arn:aws:iam::123456789012:user/invalid_user" },
      "Action": ["s3:DeleteObject"],
      "Resource": ["arn:aws:s3:::{{ bucket_name }}/*"]
    }
  ]
}